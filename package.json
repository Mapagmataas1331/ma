	"description": "Mapagmataas' website",
	"author": "Mapagmataas",
	"homepage": "https://ma.cyou",
	"repository": "github:mapagmataas1331/ma",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"postbuild": "npx svelte-sitemap --domain https://ma.cyou --out-dir build",
		"deploy": "gh-pages -d build -t true",
		"preview": "vite preview",
		"preview:gh": "http-server build",
		"validate": "npm run check && prettier --write . && eslint .",
		"test": "npm run test:integration && npm run test:unit && npm run test:e2e && npm run test:unit -- --run",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@internationalized/date": "^3.5.6",
